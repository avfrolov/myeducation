<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:annox="http://annox.dev.java.net"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
                    http://annox.dev.java.net "
        version="2.1">
    <!--jaxb:extensionBindingPrefixes="xjc annox"-->

    <jaxb:bindings schemaLocation="rules.xsd" node="/xs:schema">
        <jaxb:schemaBindings >
            <jaxb:package name="org.myeducation.taskexecuter.core.processor.circuit.jaxb.rules"></jaxb:package>
        </jaxb:schemaBindings>

        <jaxb:bindings node="xs:complexType[@name='Rules']">
            <annox:annotate>
                <annox:annotate annox:class="javax.xml.bind.annotation.XmlRootElement" name="rules"/>
            </annox:annotate>
        </jaxb:bindings>
    </jaxb:bindings>

</jaxb:bindings>